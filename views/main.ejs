<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Students</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">  
</head>
<body>
    <div class="container">
        <div class="header mt-5 mb-5 d-flex w-100 justify-content-between">
            <h3>List of Students</h3>
            <div class="right_header_section">
                <button class="btn btn-dark">
                    <a  style="text-decoration: none; color: white;" href="/List_of_Interviews">List of Interviews</a>
                </button>
                <button class="btn btn-dark">
                    <a  style="text-decoration: none; color: white;" href="/AddNewListPage">Add New Student</a>
                </button>
                <button class="btn btn-dark">
                    <a  style="text-decoration: none; color: white;" href="/Logout">Logout</a>
                </button>
            </div>
        </div>
        <div class="table-responsive">
            <table class="table table-bordered table-hover">
                <thead class="thead-dark">
                    <tr>
                        <th>S.no</th>
                        <th>Student_Name</th>
                        <th>College_Name</th>
                        <th>College_Status</th>
                        <th>DSA_Final_Score</th>
                        <th>WebD_final_Score</th>
                        <th>React_Final_Score</th>
                        <th>Schedule an Interview</th>
                    </tr>
                </thead>
                <tbody>
                    <% Student_Data.forEach(function(data, index) { %>
                        <tr>
                            <td><%= index + 1 %></td>
                            <td><%= data.Student_Name %></td>
                            <td><%= data.Student_Details.College || 'N/A' %></td>
                            <td><%= data.Student_Details.Status || 'N/A' %></td>
                            <td><%= data.Course_Score.DSA_Final_Score || 'N/A' %></td>
                            <td><%= data.Course_Score.WebD_final_Score || 'N/A' %></td>
                            <td><%= data.Course_Score.React_Final_Score || 'N/A' %></td>
                            <td><a href="/Schedule_an_Interview_/<%= data._id %>">Schedule an Interview</a></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
</body>
</html>
